{
  "name": "pokemon",
  "version": "1.0.0",
  "description": "Ein kleines pixel-art Spiel, inspiriert nach Pok√©mon",
  "scripts": {
    "test": "jest 'src/**/**/__tests__/**/*.test.ts' --passWithNoTests",
    "test:watch": "jest '' --watch --passWithNoTests",
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext js",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run type-check"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikareich/poke-mon.git"
  },
  "author": "Mika Reich, Marco Zillgen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikareich/poke-mon/issues"
  },
  "homepage": "https://github.com/mikareich/poke-mon#readme",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-loader": "^8.2.5",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "uuid": "^8.3.2"
  }
}
