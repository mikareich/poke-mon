{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/no-unresolved": "off",
    "import/extensions": "off"
  },
  "ignorePatterns": ["node_modules/", "dist/", "webpack.config.js"]
}
